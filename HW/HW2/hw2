public class hw2{
  public int findBadVersion(int n){
    int first = 1;
    int last = n;

    while (first < last){
      int middle = (first + last) / 2;
      if (isBadVersion(middle)){
        right = mid;
      } else { 
        left = mid + 1;
      }
    }
    return left;
  }
}

def findBadVersion(int n):
  first = 1
  last = n

  while first < last:
    middle = (first + last) / 2
    if isBadVersion(middle):
      last = middle
    else:
      first = middle + 1
  return left
